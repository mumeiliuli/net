@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>秘密花园</title>
    @Html.RenderCss("Content/lib/bootstrap/bootstrap.css")
    @Html.RenderCss("Content/main/login.css")
</head>
<body>
    <div class="login_main">
        <input type="hidden" value="@Request["ReturnUrl"]" id="return_url"/>
        <form class="login_form" role="form">
            <h3 class="title">秘密花园帐号登录</h3>
            <div class="input-group">
                <input type="text" class="form-control" placeholder="用户名" name="username" value="llm">
            </div>
            <div class="input-group">
                <input type="password" class="form-control" placeholder="密码" name="password" value="llm">
            </div>
            <div class="input-group">
                <input type="submit" class="form-control btn-info" value="登录">
            </div>
            <div class="input-group">
                <label class="login_error"></label>
            </div>
        </form>
    </div>
  <div class="login_bg"></div>
  @Html.RenderJs("Scripts/lib/jquery/jquery-1.10.2.js", "Scripts/lib/jquery/jquery.validate.js")
  @Html.RenderJs("Scripts/main/login.js")
</body>
</html>

